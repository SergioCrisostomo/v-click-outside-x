{
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": false,
        "modules": "commonjs",
        "spec": true,
        "targets": {
          "browsers": ["> 1%", "Explorer >= 9"]
        },
        "useBuiltIns": false,
        "debug": false
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-property-mutators",
    "@babel/plugin-proposal-object-rest-spread",
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": false,
        "helpers": false,
        "regenerator": false,
        "useESModules": false
      }
    ]
  ]
}
