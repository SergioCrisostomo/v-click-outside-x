{
  "name": "v-click-outside-x",
  "version": "4.0.1",
  "description": "Vue directive to react on clicks outside an element.",
  "homepage": "https://github.com/Xotic750/v-click-outside-x.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/Xotic750/v-click-outside-x.git"
